
namespace HotelCheckIn_InterfaceSystem.model
{
    public class RegisterInfo
    {
        private string request_id;              //request_id

        private string register_id;             //登记单ID
        private string register_type;           //登记单类型
        private string room_type;               //房间类型
        private string room_no;                 //房间号
        private string guest_names;             //客人姓名
        private string check_in_date;           //入住日期
        private string check_out_date;          //离店日期
        private string note;                    //登记单备注

        private ReturnInfo return_info;         //return_info

        /// <summary>
        /// request_id
        /// </summary>
        public string request_Id
        {
            get { return this.request_id; }
            set { this.request_id = value; }
        }
        /// <summary>
        /// 登记单ID
        /// </summary>
        public string register_Id
        {
            get { return this.register_id; }
            set { this.register_id = value; }
        }
        /// <summary>
        /// 登记单类型
        /// </summary>
        public string register_Type
        {
            get { return this.register_type; }
            set { this.register_type = value; }
        }
        /// <summary>
        /// 房间类型
        /// </summary>
        public string room_Type
        {
            get { return this.room_type; }
            set { this.room_type = value; }
        }
        /// <summary>
        /// 房间号
        /// </summary>
        public string room_No
        {
            get { return this.room_no; }
            set { this.room_no = value; }
        }
        /// <summary>
        /// 客人姓名
        /// </summary>
        public string guest_Names
        {
            get { return this.guest_names; }
            set { this.guest_names = value; }
        }
        /// <summary>
        /// 入住日期
        /// </summary>
        public string check_In_Date
        {
            get { return this.check_in_date; }
            set { this.check_in_date = value; }
        }

        /// <summary>
        /// 离店日期
        /// </summary>
        public string check_Out_Date
        {
            get { return this.check_out_date; }
            set { this.check_out_date = value; }
        }

        /// <summary>
        /// 登记单备注
        /// </summary>
        public string Note
        {
            get { return this.note; }
            set { this.note = value; }
        }

        /// <summary>
        /// 总费用
        /// </summary>
        public double TotalFee { get; set; }

        /// <summary>
        /// 总消费
        /// </summary>
        public double TotalConsume { get; set; }

        /// <summary>
        /// 房价代码
        /// </summary>
        public string RoomRateCode { get; set; }

        /// <summary>
        /// 房价
        /// </summary>
        public double RoomRate { get; set; }

        /// <summary>
        /// return_info
        /// </summary>
        public ReturnInfo return_Info
        {
            get { return this.return_info; }
            set { this.return_info = value; }
        }

    }
}
