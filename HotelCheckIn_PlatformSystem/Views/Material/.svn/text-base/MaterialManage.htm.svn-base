<body class="easyui-layout">
    <div data-options="region:'center',title:'素材管理>>素材管理'">
        <table id="dg_material">
        </table>
    </div>
    <div id="tb" style="padding: 5px; height: 30px;">
        <div style="margin-bottom: 5px; float: left;">
            <table>
                <tr>
                    <td style="text-align: right; width: 800px;">
                        <a onclick="add_click('add')" class="easyui-linkbutton" plain="true" iconcls="icon-add">
                            添 加</a>
                    </td>
                    <td>
                        <a onclick="edit_click('edit')" class="easyui-linkbutton" plain="true" iconcls="icon-edit">
                            修 改</a>
                    </td>
                    <td>
                        <a onclick="del_click()" class="easyui-linkbutton" plain="true" iconcls="icon-remove">
                            删 除</a>
                    </td>
                    <td>
                        <a onclick="upload_click()" class="easyui-linkbutton" plain="true" iconcls="icon-upload">
                            上传文件</a>
                    </td>
                </tr>
            </table>
        </div>
    </div>
    <div id="dlg" class="easyui-dialog">
        <div id="swfu_container" style="margin: 0 0px 10px 0;">
            <div>
                <span id="spanButtonPlaceholder"></span>
            </div>
            <!-- <div id="divFileProgressContainer" style="height: 75px;">
                 </div>
                <!--<div id="thumbnails"></div>-->
        </div>
        <div style="margin: 0; padding: 0 1px; height: 300px;">
            <table id="dg_file">
            </table>
        </div>
    </div>
    <div id="dlg_material" class="easyui-dialog">
        <div id="divFileProgressContainer">
        </div>
    </div>
    <div id="dlg_form" class="easyui-dialog">
        <form name="documentForm" method="post" id="documentForm">
        <div class="tableForm">
            <table border="1" style="border: 1px solid #dcdcdc;" cellspacing="1" cellpadding="1">
                <tr class="tr">
                    <td class="td">
                        名称:
                    </td>
                    <td class="td-input">
                        <input name="txt_name" id="txt_name" type="text" class="easyui-validatebox txt" required="true" validtype="name" />
                    </td>
                    <td class="redstart">
                    </td>
                </tr>
                <tr class="tr">
                    <td class="td">
                        备注:
                    </td>
                    <td class="td-input">
                        <textarea name="txt_note" id="txt_note" cols="26" rows="4">
                    </textarea>
                    </td>
                    <td class="redstart">
                    </td>
                </tr>
            </table>
        </div>
        </form>
    </div>
